{
  "version": 6,
  "buildPresets": [
    {
      "name": "build.default.minsizerel",
      "configurePreset": "config.default.minsizerel",
      "configuration": "MinSizeRel"
    },
    {
      "name": "build.default.release",
      "configurePreset": "config.default.release",
      "configuration": "Release"
    },
    {
      "name": "build.default.debug",
      "configurePreset": "config.default.debug",
      "configuration": "Debug"
    },
    {
      "name": "build.vs-clang.minsizerel",
      "configurePreset": "config.vs-clang.minsizerel",
      "configuration": "MinSizeRel"
    },
    {
      "name": "build.vs-clang.release",
      "configurePreset": "config.vs-clang.release",
      "configuration": "Release"
    },
    {
      "name": "build.vs-clang.debug",
      "configurePreset": "config.vs-clang.debug",
      "configuration": "Debug"
    },
    {
      "name": "build.tests",
      "configurePreset": "config.tests",
      "configuration": "Debug"
    },
    {
      "name": "build.profile",
      "configurePreset": "config.profile",
      "configuration": "Debug"
    }
  ],
  "configurePresets": [
    {
      "hidden":true,
      "name": "_setup.common",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "hidden": true,
      "name": "_setup.toolchain.default"
    },
    {
      "hidden": true,
      "name": "_setup.toolchain.vs-clang",
      "toolset": "ClangCL",
      "cacheVariables": {
        "LLVM_ENABLE_RUNTIMES": "libcxx",
        "LIBCXX_ENABLE_SHARED": {
          "type": "BOOL",
          "value": "YES"
        },
        "LIBCXX_ENABLE_STATIC": {
          "type": "BOOL",
          "value": "NO"
        }
      }
    },
    {
      "hidden": true,
      "name": "_setup.config.minsizerel",
      "cacheVariables": {
        "sk_build_opensk": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_build_tests": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_pedantic_exceptions": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_debug_output": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_profiling": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_physics_debug": {
          "type": "BOOL",
          "value": "OFF"
        }
      }
    },
    {
      "hidden": true,
      "name": "_setup.config.release",
      "cacheVariables": {
        "sk_build_opensk": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_build_tests": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_pedantic_exceptions": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_debug_output": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_profiling": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_physics_debug": {
          "type": "BOOL",
          "value": "OFF"
        }
      }
    },
    {
      "hidden": true,
      "name": "_setup.config.debug",
      "cacheVariables": {
        "sk_build_opensk": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_build_tests": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_pedantic_exceptions": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_debug_output": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_profiling": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_physics_debug": {
          "type": "BOOL",
          "value": "OFF"
        }
      }
    },
    {
      "hidden": true,
      "name": "_setup.config.tests",
      "cacheVariables": {
        "sk_build_opensk": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_build_tests": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_pedantic_exceptions": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_debug_output": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_profiling": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_physics_debug": {
          "type": "BOOL",
          "value": "OFF"
        }
      }
    },
    {
      "hidden": true,
      "name": "_setup.config.profile",
      "cacheVariables": {
        "sk_build_opensk": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_build_tests": {
          "type": "BOOL",
          "value": "OFF"
        },
        "sk_pedantic_exceptions": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_debug_output": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_profiling": {
          "type": "BOOL",
          "value": "ON"
        },
        "sk_physics_debug": {
          "type": "BOOL",
          "value": "ON"
        }
      }
    },
    {
      "name": "config.default.minsizerel",
      "displayName": "Default Release",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/default/minsizerel",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.default",
        "_setup.config.minsizerel"
      ]
    },
    {
      "name": "config.default.release",
      "displayName": "Default Release",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/default/release",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.default",
        "_setup.config.release"
      ]
    },
    {
      "name": "config.default.debug",
      "displayName": "Default Debug",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/default/debug",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.default",
        "_setup.config.debug"
      ]
    },
    {
      "name": "config.vs-clang.minsizerel",
      "displayName": "ClangCL MinSizeRel",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/vs-clang/minsizerel",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.vs-clang",
        "_setup.config.minsizerel"
      ]
    },
    {
      "name": "config.vs-clang.release",
      "displayName": "ClangCL Release",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/vs-clang/release",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.vs-clang",
        "_setup.config.release"
      ]
    },
    {
      "name": "config.vs-clang.debug",
      "displayName": "ClangCL Debug",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/vs-clang/debug",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.vs-clang",
        "_setup.config.debug"
      ]
    },
    {
      "name": "config.tests",
      "displayName": "Configure tests",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/test",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.default",
        "_setup.config.tests"
      ]
    },
    {
      "name": "config.profile",
      "displayName": "Configure profiling",
      "binaryDir": "${sourceDir}/cmake-build/${hostSystemName}/profile",
      "inherits": [
        "_setup.common",
        "_setup.toolchain.default",
        "_setup.config.profile"
      ]
    }
  ],
  "testPresets": [
    {
      "hidden": true,
      "name": "_test.common",
      "output": {
        "verbosity": "verbose",
        "outputOnFailure": true,
        "outputLogFile": "ctest_output.log"
      },
      "execution": {
        "noTestsAction": "error",
        "stopOnFailure": false
      }
    },
    {
      "name": "test.default",
      "configurePreset": "config.tests",
      "configuration": "debug",
      "inherits": [
        "_test.common"
      ]
    }
  ],
  "workflowPresets": [
    {
      "name": "release-build",
      "steps": [
        {
          "name": "config.default.release",
          "type": "configure"
        },
        {
          "name": "build.default.release",
          "type": "build"
        }
      ]
    },
    {
      "name": "run-tests",
      "steps": [
        {
          "name": "config.tests",
          "type": "configure"
        },
        {
          "name": "build.tests",
          "type": "build"
        },
        {
          "name": "test.default",
          "type": "test"
        }
      ]
    }
  ]
}